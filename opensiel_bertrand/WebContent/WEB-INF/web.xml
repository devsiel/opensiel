<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>OpenSIEL</display-name>
<display-name>OpenSIEL1.0</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
		<servlet-name>creationkitmin</servlet-name>
		<servlet-class>opensiel.servlets.creationkitmin</servlet-class>
  </servlet>
  <servlet-mapping>
		<servlet-name>creationkitmin</servlet-name>
		<url-pattern>/creationkitmin</url-pattern>
  </servlet-mapping>
  
  <servlet>
		<servlet-name>upload</servlet-name>
		<servlet-class>opensiel.servlets.UploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
		<servlet-name>upload</servlet-name>
		<url-pattern>/upload</url-pattern>
  </servlet-mapping>
  
</web-app>